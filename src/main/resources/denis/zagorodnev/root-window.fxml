<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<TabPane prefHeight="2160.0" prefWidth="3840.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="denis.zagorodnev.controller.englishtab.EnglishTabController">

    <Tab text="Английский">
        <AnchorPane minHeight="0.0" minWidth="0.0">
         <Label text="В разработке!" textFill="#ed4949" AnchorPane.bottomAnchor="20" AnchorPane.rightAnchor="50">
            <font>
               <Font name="System Bold Italic" size="24.0" />
            </font>
         </Label>
            <Pane fx:id="rootPane" prefHeight="469.0" prefWidth="845.0">
                <TableView fx:id="topTableWord" layoutX="351.0" layoutY="124.0" prefHeight="315"
                           style="-fx-font-style: italic; -fx-font-size: 14px;">
                    <columns>
                        <TableColumn fx:id="translation" prefWidth="150" style="-fx-alignment: center;" text="Перевод"/>
                        <TableColumn fx:id="original" prefWidth="150" style="-fx-alignment: center;" text="Оригинал"/>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                </TableView>
                <Button layoutX="53.0" layoutY="13.0" mnemonicParsing="false" onAction="#addWord"
                        text="Добавить слово"/>
                <Button layoutX="180.0" layoutY="13.0" mnemonicParsing="false" onAction="#passTest"
                        text="Пройти тест (откроется окно с вкладками слова, предложения)"/>
            </Pane>
        </AnchorPane>
    </Tab>

    <Tab text="Математика">
        <AnchorPane minHeight="0.0" minWidth="0.0">
            <Label layoutX="278.0" layoutY="183.0" text="В разработке!!!" textFill="#eb3232">
                <font>
                    <Font name="System Bold Italic" size="52.0"/>
                </font>
            </Label>
        </AnchorPane>
    </Tab>

</TabPane>
